          <div style="text-align: center;">
            <select class="selectpicker" style="text-align-last: center;" name="model">
              <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(rows){
        %>
              <option value="<%=rows.model %>"><%=rows.model %></option>
              <% }) %>
              <% }%>
            </select>
          </div>
          <div style="font-weight:bold; margin: 10px;text-align: center;">Year:</div>
          <div style="text-align: center;">
            <select class="selectpicker" style="text-align-last: center;" name="year">
